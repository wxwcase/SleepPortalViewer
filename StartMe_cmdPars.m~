function StartMe_cmdPars(varargin) %varargin
%varargin: (edfPath, edfName, xmlPath, xmlName)
    
    global needOpenDialog;
    global FilePath;
    global FileName;
    global XmlFilePath;
    global XmlFileName;
    
    if length(varargin) ~= 4
        needOpenDialog = TRUE;
    else
        needOpenDialog = FALSE;
        FilePath = varargin{1};
        FileName = varargin{1};
        XmlFilePath = varargin{1};
    XmlFileName = varargin{1};
    end
    
   % global EdfFilePath;
   % global EdfFileName;
    %EdfFilePath = edfName;
    %EdfFileName = edfPath;
    
    FilePath = varargin{1};
    FileName = varargin{1};
    XmlFilePath = varargin{1};
    XmlFileName = varargin{1};
    
%     FilePath = edfName;
%     FileName = edfPath;
%     XmlFilePath = xmlName;
%     XmlFileName = xmlPath;
    
    EDF_View({})
end